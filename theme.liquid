div.bells_top_row.shown {
    transition: all 2s 0.5s ease-in-out;
    height:auto;
}
div.bells_top_row .bell {
    transform:translateY(20rem) scale(0) rotateX(90deg);
    opacity:0;
    transition: all 0.5s ease-in-out;
    flex-basis:9%;
    margin-top:-.3%;
    /* margin-bottom:14%; */
}
