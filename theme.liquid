    transform:translateY(2rem) scale(1) rotateX(0);
    opacity:1;
    height:auto;
}
div.bells_main_row {
    display:flex;
    flex-direction:row;
    justify-content:flex-start;
    align-items:center;
    column-gap: 0.5%;
